{
  "name": "ng-hello-world",
  "version": "1.0.0-alpha.0",
  "description": "ng-hello-world",
  "keywords": "Angular hello world",
  "author": "Marc Laval",
  "scripts": {
    "clean": "rm -rf dist/",
    "public": "mkdir dist && cp -a public/* dist/",
    "build": "npm run clean && npm run public && webpack --config webpack.config.js --progress --profile --bail",
    "start": "http-server ./dist -s"
